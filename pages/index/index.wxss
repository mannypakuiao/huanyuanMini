  margin: auto;
  /* padding: 20rpx; */
  box-sizing: border-box;
}
.ticketorder_content image{
  width: 100%;
  height: 100%;
}
.one{
  height: 180rpx;
  /* background: rgba(193, 224, 163, 0.678); */
