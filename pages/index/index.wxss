.times{
  color: #867563;
  font-size: 33rpx;
}
.ticketorder_title{
  display: inline-block;
  height: 80rpx;
  line-height: 50rpx;
  margin-left: 20rpx;
}
.titles{
  color: #5d3f23;
  font-size: 45rpx;
  letter-spacing: 4rpx;
}
.jiage{
  margin-left: 50rpx;
  font-size: 30rpx;
  margin-top: 10rpx;
}
/* 景区介绍 */
.jieshao{
  display: flex;
  justify-content: space-between;
  border-bottom: 2rpx solid rgb(224, 224, 224);
  box-sizing: border-box;
  padding: 40rpx 20rpx;  
}
.type_name{
  color: #5d3f23;
  font-size: 40rpx;
}
.titles_name{
  font-size: 33rpx;
  margin-top: 20rpx;
  letter-spacing: 4rpx;
}
.type_desc{
  font-size: 17rpx;
  display: flex;
  justify-content: flex-start;
  margin-top: 10rpx;
}
.desc{
  color: #bfbfbf;
  border: 2rpx solid #bfbfbf;
  border-radius: 5rpx;
  padding: 4rpx;
}
.desc:nth-child(2){
  margin-left: 10rpx;
}
.xuzhi{
  font-size: 24rpx;
  color: rgb(129, 129, 129);
  margin-left: 20rpx;
}
.menpiao_right{
  position: relative;
}
.right_ront{
  position: absolute;
  bottom: 0;
  right: 0;
}
.shiji_money{
  color: red;
  text-align: right;
}
.yuanlai_moneg{
  text-decoration: line-through;
  color: rgb(148, 148, 148);
  font-size: 24rpx;
  text-align: right;
  margin-top: 5rpx;
}
.shopping{
  color: #fff;
  background: #c8a468;
  padding: 13rpx 24rpx;
  font-size: 33rpx;
  text-align: center;
  border-radius: 5rpx;
  margin-top: 27rpx;
  width: 120rpx;
  float: right;
  line-height: 33rpx;
}
/* 景区介绍 */
.jingqu_jieshao{
  padding: 20rpx;
}
.jieshao_title{
  font-size: 40rpx;
  color: #5d3f23;
}
text{
  font-size: 27rpx;
  line-height: 52rpx;
}
.xuzhi_content{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-top: 20rpx;
}
